# SSP NFC Configurator Environment Configuration
# Copy this file to .env and fill in your values

# GraphQL API Configuration
API_URL=https://api.yourserver.com/graphql
API_TIMEOUT=30000

# NFC Tag URL Configuration
# Base URL that will be written to NFC tags (e.g., https://app.yourserver.com/table)
NFC_BASE_URL=https://app.yourserver.com/table

# Feature Flags
ENABLE_DEBUG_MODE=false
ENABLE_MOCK_NFC=false

# App Configuration
APP_NAME=SSP NFC Config
APP_VERSION=1.0.0
