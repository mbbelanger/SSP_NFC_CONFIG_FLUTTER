# SSP NFC Registration Production Environment Configuration
#
# Copy this file to .env before building for production:
#   cp .env.production.example .env
#   flutter build appbundle --release
#
# IMPORTANT: Verify endpoints are live and SSL-configured before release

# GraphQL API Configuration
API_URL=https://api.ssppos.com/graphql
API_TIMEOUT=30000

# NFC Tag URL Configuration
NFC_BASE_URL=https://splt.ca/table

# Feature Flags
# CRITICAL: ENABLE_MOCK_NFC must be false for production
ENABLE_DEBUG_MODE=false
ENABLE_MOCK_NFC=false

# App Configuration
APP_NAME=SSP NFC Registration
APP_VERSION=1.0.0
